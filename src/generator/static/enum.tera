{%- macro render(model) -%}
{{- model.doc | comment }}
#[derive(Debug, Serialize, Deserialize, Clone, PartialEq, Eq)]
pub enum {{ model.title }} {
    {%- for variant in model.variants %}
    #[serde(rename = {{ variant.value | quote }})]
    {{ variant.title }},
    {%- endfor %}
}
{%- endmacro render -%}
