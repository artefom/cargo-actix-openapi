{%- macro render(model) -%}
{{- model.doc | comment -}}
#[derive(Serialize, Deserialize, Clone, PartialEq)]
pub struct {{model.title}} {
    {%- for prop in model.props %}
    {{- prop.annotation | indent(n=4) }}
    pub {{ prop.title }}: {{ prop.type_ }},
    {%- endfor %}
}
{%- endmacro render -%}
